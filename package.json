{
  "name": "react-equal-height",
  "version": "1.2.1",
  "description": "Compares element heights and sets the highest (based on react-hooks)",
  "scripts": {
    "build": "webpack --mode=production --config webpack.prod.js && webpack --mode=production --config webpack.prod-clean.js && cp README.md dist/README.md && rm -f ./dist/app.d.ts && cat ./package.json | grep -v '\"private\": ' > dist/package.json && npm pack ./dist",
    "server": "webpack-dev-server --port 8889 --config webpack.dev.js",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config ../jest.config.json",
    "test:watch": "jest --config ../jest.config.json --watch",
    "test:coverage": "jest --config ../jest.config.json --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loba-b/react-equal-height.git"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com/loba-b"
  },
  "private": true,
  "keywords": [
    "react",
    "reactjs",
    "equal",
    "height",
    "react-hooks",
    "hooks",
    "typescript",
    "useEffect",
    "useContext"
  ],
  "engines": {
    "node": ">=12",
    "react": ">=16"
  },
  "author": "Bartosz Loba",
  "license": "MIT",
  "bugs": "https://github.com/loba-b/react-equal-height/issues",
  "homepage": "https://github.com/loba-b/react-equal-height#readme",
