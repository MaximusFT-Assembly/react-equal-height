{
  "testRegex": "((\\.|/*.)(spec))\\.js?$",
  "setupFiles": [
    "./setupTests.js"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "moduleNameMapper": {
    "^.+\\.(css|scss)$": "identity-obj-proxy"
  },
  "transform": {
    "^.+\\.(js|jsx|tsx)$": "babel-jest"
  },
  "testEnvironment": "jsdom"
}
